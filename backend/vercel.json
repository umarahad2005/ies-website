{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "server.js"
    }
  ],
  "env": {
    "EMAIL_SERVICE": "@email_service",
    "EMAIL_USER": "@email_user",
    "EMAIL_PASSWORD": "@email_password",
    "COMPANY_EMAIL": "@company_email",
    "FRONTEND_URL": "@frontend_url",
    "NODE_ENV": "production"
  }
}
